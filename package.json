{
  "name": "airdcpp-webui",
  "version": "2.2.0",
  "description": "AirDC++ Web UI",
  "license": "MIT",
  "authors": [
    "maksis"
  ],
  "keywords": [
    "airdcpp",
    "airdcpp-webapi",
    "react"
  ],
  "engines": {
    "node": ">=4"
  },
  "bugs": {
    "url": "https://github.com/airdcpp-web/airdcpp-webui/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/airdcpp-web/airdcpp-webui.git"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "eslint --ext .jsx,.js src",
    "lint-fix": "eslint --ext .jsx,.js --fix src",
    "start": "node server.js",
    "build-webpack": "webpack -p --progress",
    "build": "npm run clean && npm run build-webpack",
    "build-debug": "npm run clean && webpack --progress",
    "test": "jest",
    "prepublish": "npm run build"
  },
