    "jquery": "^3.1.0",
    "lodash": "^4.6.0",
    "minimist": "^1.2.0",
    "moment": "^2.12.0",
    "pondjs": "^0.8.7",
    "promise": "^8.0.1",
    "prop-types": "^15.5.4",
    "react": "^16.0.0-rc",
