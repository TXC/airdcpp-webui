{
  "name": "airdcpp-webui",
  "version": "0.0.1",
  "description": "AirDC++ Web UI",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:airdcpp/airgit.git"
  },
  "authors": [
    "maksis <maksis@adrenaline-network.com> (http://www.airdcpp.net)"
  ],
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "lint": "eslint src",
    "start": "npm run build && serve .",
    "build": "npm run build-js && npm run build-css",
    "watch": "npm run watch-js & npm run watch-css & serve .",
    "test": "npm run lint -s && npm run build",
    "build-css": "rework-npm index.css | cleancss -o build/build.css",
    "build-js": "browserify --extension=.jsx --extension=.js src/app.jsx | uglifyjs > build/build.js",
    "watch-js": "watchify --extension=.jsx --extension=.js src/app.jsx -o build/build.js --debug --verbose",
    "watch-css": "nodemon -e css --ignore build/build.css --exec 'rework-npm index.css -o build/build.css'",
    "lint-eslint": "eslint .",
    "lint-jscs": "jscs ."
  },
  "dependencies": {
    "bluebird": "^2.9.34",
    "classnames": "^2.1.3",
	"fixed-data-table": "^0.4.6",
	"jquery"  : "^2.1.4",
    "lodash": "^3.10.0",
    "normalize.css": "3.0.3",
    "moment": "^2.10.6",
    "react": "^0.13",
	"react-autosuggest": "1.18.3",
    "react-mixin": "^1.1.0",
	"react-portal": "^0.7.0",
    "react-router": "^1.0.0-rc1",
    "react-semantify": "^0.3.1",
    "reflux": "^0.2.12",
    "semantic-ui": "^2.0.7",
    "superagent": "^1.2.0",
    "tcomb-form": "^0.6.3"
  },
  "devDependencies": {
    "babel-core": "^5.8.23",
    "babel-eslint": "^4.1.1",
    "babel-loader": "^5.3.2",
    "babelify": "^6.1.0",
    "browserify": "^8.0.3",
    "clean-css": "^3.1.9",
    "css-loader": "^0.15.6",
    "cssnext": "^1.8.2",
    "eslint": "^1.3.1",
	"eslint-plugin-react ": "^3.3.1",
    "jscs": "^2.1.1",
    "minimist": "^1.1.2",
    "postcss": "^4.1.16",
    "postcss-loader": "^0.5.1",
    "postcss-nested": "^0.3.2",
    "psi": "^1.0.6",
    "rework": "^1.0.1",
    "rework-npm": "^1.0.0",
    "rework-npm-cli": "^0.1.1",
    "style-loader": "^0.12.3",
    "uglify-js": "^2.4.15",
    "watchify": "^2.1.1"
  }
}
