    "react-notification-system": "^0.2.15",
    "react-resizable-box": "^2.1.0",
    "react-router": "^3.0.0",
    "react-select": "1.0.0-rc.5",
    "react-timeseries-charts": "^0.12.8",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.4",
    "react-transition-group": "^2.2.0",
